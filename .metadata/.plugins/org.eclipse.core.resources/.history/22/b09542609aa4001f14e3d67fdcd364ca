
public class Main {

	public static void main(String[] args) {
		Gear g1, g2, g3;
		g1 = g2 = g3 = new Gear();
		g1.setDiameter(32);
		g1.setCogs(64);
		g2.setDiameter(32);
		g2.setCogs(64);
		g3.setDiameter(32);
		g3.setCogs(64);
		
		Machine m = new Machine("Machine02",g1,g2,g3);
		System.out.println(m);
	}
}
